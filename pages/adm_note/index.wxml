<view class="home" >
    <view class="one" wx:for="{{data}}">
      <view class="title">{{item.title}}</view>
      <view class="content"><text space="ensp">  {{item.content}}</text></view>
      <button  data-index='{{item.content}}' style="font-size:35rpx !important;width: 80px;margin-left: 60px;" bindtap="del" type="warn" size="mini">删除</button>
      <view class="time">{{item.time}}</view>
    </view>
</view>